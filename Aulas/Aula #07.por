programa {
  funcao inicio() {
   /* Aula de Condicionais (se, senaose, senao) */
   cadeia nome
   inteiro dinheiro
  escreva("\n Digite o nome do usuário: ")
      leia(nome)
  escreva("\n Digite o valor do dinheiro: R$")
      leia(dinheiro)
  se (dinheiro>=10000){ 
    escreva("\n Partiu Disney \n")}
  senao {
  escreva("\n Tem que trabalhar mais! \n")}
  }
}
