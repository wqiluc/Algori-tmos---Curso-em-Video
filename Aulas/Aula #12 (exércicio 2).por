programa {
  
  funcao parimpar(inteiro numero){
    escreva("\n Digite um número: ")
    leia(numero)
    se (numero % 2 == 0){
      escreva("\n Esse número é: PAR!!")} 
    senao{
      escreva("\n Esse número é: ÍMPAR!!")}
  }

  funcao inicio() {
    inteiro numero
    escreva("\n Digite um número: ")
    leia(numero)
    se (numero % 2 == 0){
      escreva("\n Esse número é: PAR!!")} 
    senao{
      escreva("\n Esse número é: ÍMPAR!!")}
  }
}
